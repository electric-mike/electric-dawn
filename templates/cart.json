{
  "sections": {
    "cart-customize": {
      "type": "cart-customize",
      "settings": {
        "enable_flex_cart_layout": true,
        "cart_width": 70,
        "subtotal_checkout_width": 30
      }
    },
    "cart-items": {
      "type": "main-cart-items",
      "settings": {
        "show_vendor": true
      }
    },
    "cart-footer": {
      "type": "main-cart-footer",
      "blocks": {
        "subtotal": {
          "type": "subtotal",
          "settings": {}
        },
        "free-shipping-bar": {
          "type": "free-shipping-bar",
          "settings": {}
        },
        "buttons": {
          "type": "buttons",
          "settings": {}
        }
      },
      "block_order": [
        "subtotal",
        "free-shipping-bar",
        "buttons"
      ],
      "settings": {
        "show_cart_note": false,
        "free_shipping_threshold": 100
      }
    },
    "featured-products": {
      "type": "featured-collection",
      "settings": {
        "title": "You might also like",
        "collection": "all",
        "products_to_show": 4,
        "show_view_all": true,
        "swipe_on_mobile": true,
        "image_ratio": "square",
        "show_secondary_image": false,
        "add_image_padding": false,
        "show_image_outline": true,
        "show_vendor": false,
        "show_rating": false
      }
    }
  },
  "order": [
    "cart-customize",
    "cart-items",
    "cart-footer",
    "featured-products"
  ]
}